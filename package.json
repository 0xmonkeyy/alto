{
    "name": "alto",
    "version": "0.0.1",
    "description": "A performant and modular ERC-4337 Bundler written in Typescript",
    "main": "src/run.ts",
    "repository": "https://github.com/pimlicolabs/alto.git",
    "author": "Pimlico",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "build": "tsc",
        "start": "node dist/cli/alto.js run",
        "start:dev": "ts-node src/cli/alto.ts",
        "test": "mocha",
        "test:integration": "pdm install && pdm run update-deps && pdm run pytest -rA -W ignore::DeprecationWarning --url <bundler_url> --entry-point <entry_point> --ethereum-node <ethereum_node_url> --launcher-script <launcher-script-file>",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "devDependencies": {
        "@swc/core": "^1.3.49",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/mocha": "^10.0.1",
        "@types/yargs": "^17.0.24",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "earl": "^1.1.0",
        "eslint": "^8.38.0",
        "eslint-config-es": "^4.2.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.8.7",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "mongodb": "^5.2.0",
        "viem": "^0.3.2",
        "winston": "^3.8.2",
        "yargs": "^17.7.1",
        "zod": "^3.21.4",
        "zod-validation-error": "^1.3.0"
    }
}
